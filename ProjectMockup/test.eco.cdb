<!doctype html>
<meta charset = "utf-8">
<title>The Forest of Lyrica</title>

<html>
  <head>
    <script type="text/javascript">

      function activateTab(pageId) {
          var tabCtrl = document.getElementById('tabCtrl');
          var pageToActivate = document.getElementById(pageId);
          for (var i = 0; i < tabCtrl.childNodes.length; i++) {
              var node = tabCtrl.childNodes[i];
              if (node.nodeType == 1) { /* Element */
                  node.style.display = (node == pageToActivate) ? 'block' : 'none';
              }
          }
      }

    </script>
  </head>
  <body>
      <tr>
        <button onclick="javascript:activateTab('page1')">Suite 1</button>
        <button onclick="javascript:activateTab('page2')">Suite 2</button>
        <button onclick="javascript:activateTab('page3')">Suite 3</button>
        <button onclick="javascript:activateTab('page4')">Suite 4</button>
        <button onclick="javascript:activateTab('page5')">Suite 5</button>
      </tr>
